{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "benbenek/multi-ci-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "benbenek/multi-ci-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "benbenek/multi-ci-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "memory": 128
        }
    ]
}