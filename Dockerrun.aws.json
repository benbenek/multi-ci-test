{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "benbenek/multi-ci-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "benbenek/multi-ci-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "benbenek/multi-ci-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "benbenek/multi-ci-nginx",
            "hostname": "hostname",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": "80",
                    "containerPort": "80"
                }
            ],
            "links": [
                "client",
                "server"
            ]
        }
    ]
}